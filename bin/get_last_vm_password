#!/bin/bash
export NAME=$1
RES=$(mysql cortex -NBqe 'select concat(`name`, "|", `build_count`, "|", `allocation_date`, "|", `allocation_who`, "|dUYgRWLKtqM6lx9jHcdJULwXZHEXtEb3vNNFx75NLyxDvXowPUiT70o1Nab888fd") from `systems` where `name`="'$NAME'" AND `vmware_uuid` IS NOT NULL')
echo -n $RES | sha256sum | cut -d' ' -f1 | xxd -r -p | base64 --wrap=0 | cut -c1-16
